{
  "name": "spa-convex",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "check-types": "bunx --bun tsc --noEmit",
    "lint": "bunx --bun eslint --cache",
    "lint:fix": "bunx --bun eslint --cache --fix",
    "format": "bunx --bun prettier . --write --cache",
    "format:check": "bunx --bun prettier . --check",
    "check": "bun run check-types && bun run lint && bun run format:check",
    "fix": "bun run check-types && bun run lint:fix && bun run format",
    "preview": "bunx --bun vite preview"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5"
  }
}
