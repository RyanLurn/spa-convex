{
  "name": "spa-convex",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "check-types": "bunx --bun tsc --noEmit",
    "lint": "bunx --bun eslint --cache",
    "lint:fix": "bunx --bun eslint --cache --fix",
    "lint:inspect": "bunx --bun eslint --inspect-config",
    "format": "bunx --bun prettier . --write --cache",
    "format:check": "bunx --bun prettier . --check",
    "check": "bun run check-types && bun run lint && bun run format:check",
    "fix": "bun run check-types && bun run lint:fix && bun run format",
    "preview": "bunx --bun vite preview"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-router": "^1.131.44",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scan": "^0.4.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tanstack/router-plugin": "^1.131.44",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bun": "^1.2.22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "lefthook": "^1.13.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tw-animate-css": "^1.3.8",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}
